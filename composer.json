{
    "name": "elife/api-validator",
    "description": "eLife Sciences API validator",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "eLife\\ApiValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "test\\eLife\\ApiValidator\\": "test/src/"
        }
    },
    "require": {
        "php": "^7.0",
        "beberlei/assert": "^2.0",
        "elife/api": "dev-master",
        "guzzlehttp/psr7": "^1.0",
        "psr/http-message": "^1.0",
        "puli/composer-plugin": "^1.0-beta9",
        "puli/repository": "^1.0-beta9",
        "webmozart/json": "^1.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.4",
        "phpspec/prophecy": "^1.5",
        "phpunit/phpunit": "^5.2"
    },
    "minimum-stability": "beta",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
